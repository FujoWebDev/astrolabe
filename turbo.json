{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["src/dist/**", "src/plugins/**/dist/**"]
    },
    "storybook": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "build-storybook": {
      "inputs": ["storybook/**/*.tsx", "storybook/**/*.ts"],
      "dependsOn": ["^build"],
      "outputs": ["storybook/storybook-static/**"]
    },
    "clean": {
      "cache": false
    }
  }
}
