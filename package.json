{
  "name": "@fujocoded/astrolabe",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "test": "turbo run test",
    "storybook": "turbo run storybook --filter=astrolabe-storybook",
    "build-storybook": "turbo run build-storybook --filter=astrolabe-storybook",
    "yalc:publish": "pnpm -r exec yalc publish"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not op_mini all"
  ],
  "devDependencies": {
    "@storybook/addon-docs": "^10.0.0-beta.9",
    "@storybook/addon-vitest": "^10.0.0-beta.9",
    "@storybook/react-vite": "catalog:storybook",
    "@types/node": "^24.6.1",
    "@types/react": "catalog:react",
    "@types/react-dom": "catalog:react",
    "tsdown": "^0.15.6",
    "turbo": "^2.5.8",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@10.0.0",
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
